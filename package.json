{
  "name": "loopback-connector-couchdb2",
  "version": "1.5.3",
  "publishConfig": {
    "export-tests": true
  },
  "description": "LoopBack Connector for CouchDB 2.0",
  "engines": {
    "node": ">=8.9"
  },
  "author": "IBM Corp.",
  "keywords": [
    "IBM",
    "StrongLoop",
    "LoopBack",
    "Couch",
    "CouchDB",
    "DataSource",
    "Connector"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean",
    "test": "lb-mocha \"test/*.test.js\"",
    "posttest": "npm run lint",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "async": "^3.2.0",
    "debug": "^4.1.1",
    "lodash": "^4.17.11",
    "loopback-connector": "^4.0.0",
    "nano": "^6.4.2",
    "request": "^2.81.0",
    "strong-globalize": "^6.0.3"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "escape-string-regexp": "1.0.5",
    "eslint-config-loopback": "^13.1.0",
    "loopback-datasource-juggler": "^3.0.0",
    "ms": "^2.0.0",
    "rc": "^1.1.5",
    "should": "^13.2.3",
    "sinon": "^11.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/strongloop/loopback-connector-couchdb2.git"
  },
  "homepage": "https://github.com/strongloop/loopback-connector-couchdb2",
  "bugs": {
    "url": "https://github.com/strongloop/loopback-connector-couchdb2/issues"
  },
  "license": "Apache-2.0"
}
